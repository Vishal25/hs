<div class="container-fluid" style="padding-top: 100px" ng-controller="HouseController as houseCard">
	<form>
		<input type="text" ng-model="newEntry.name">
	</form>
	{{newEntry.name}}
    
    
	<div class="card-deck-wrapper col-xs-12">
			<div class="card col-md-4 col-sm-6 col-xs-12" ng-repeat="product in houseCard.products">
				<img class="card-img-top" src="holder.js/100px200/" alt="Card image cap">
				<div class="card-block">
					<h4 class="card-title">{{product.name}}</h4>
					<p class="card-text">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
					<p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
				</div>
			</div>
		</div>
</div>