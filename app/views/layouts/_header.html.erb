<!-- Login Modal -->
<div class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="gridModalLabel">Signup</h4>
        </div>
        <div class="modal-body">
            <form>
                <div class="form-group">
                    <label for="customer-name" class="control-label">Name</label>
                    <input type="text" class="form-control" id="customer-name">
                </div>
                <div class="form-group">
                    <label for="customer-email" class="control-label">Email</label>
                    <input type="email" class="form-control" id="customer-email">
                </div>
                <div class="form-group">
                    <label for="customer-mobile" class="control-label">Mobile</label>
                    <input type="text" class="form-control" id="customer-mobile">
                </div>
                <div class="form-group">
                    <label for="customer-password" class="control-label">Password</label>
                    <input type="password" class="form-control" id="customer-password">
                </div>
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-primary">Signup</button>
        </div>
    </div>
  </div>
</div>
<!-- /Login Modal -->